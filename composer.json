{
    "name": "thedomeffm/monolog-dbal-handler-bundle",
    "description": "DBAl (doctrine) handler to push your logs to your database",
    "type": "symfony-bundle",
    "require": {
        "php": ">=8.1",
        "doctrine/dbal": "^3.0|^4.0",
        "monolog/monolog": "^3.0",
        "symfony/config": "^5.4|^6.0|^7.0",
        "symfony/dependency-injection": "^5.4|^6.0|^7.0",
        "symfony/http-kernel": "^5.4|^6.0|^7.0"
    },
    "config": {
        "sort-packages": true,
        "lock": false
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TheDomeFfm\\MonologDbalHandlerBundle\\": "src/"
        }
    }
}
